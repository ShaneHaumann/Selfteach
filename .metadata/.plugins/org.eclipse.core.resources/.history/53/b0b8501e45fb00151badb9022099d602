class apples{
	public static void main (String args []) {
		int age = 60;
		
		if (age <= 60)
			System.out.println("You are in 60's");
		else
			System.out.println("You are old");
			if (age > 75)
				System.out.println("You are really old");
			else
				System.out.println("You not really that old");
		}
		
	}

